{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "AusgleichslisteApp": "Debug"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff} [{Level:u3}] [{ThreadId}] {SourceContext}: {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "ApplicationSettings": {
    "Currency": "â‚¬",
    "DateCulture": "de-DE",
    "ItemsPerPage": 100,
    "ShowDebugInfo": false,
    "Branding": {
      "PrimaryColor": "#007bff",
      "SecondaryColor": "#6c757d"
    }
  },
  "AdminUser": {
    "Username": "admin",
    "Password": "ChangeMe123!",
    "DisplayName": "Administrator"
  }
}
